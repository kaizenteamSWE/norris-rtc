<!DOCTYPE html>
<html ng-app="chuck">
    <head>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16">
        <link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32">
        <link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96">

        <script src="/bower_components/angular/angular.min.js"></script>
        <script src="/bower_components/jquery/dist/jquery.min.js"></script>
        <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
        <script src="/node_modules/chuck-rtc/chuck.js"></script>

        <link href="/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
        <link href="/css/custom.css" rel="stylesheet">
    </head>
    <body>

    <!-- Main jumbotron for a primary marketing message or call to action -->
    <header class="jumbotron text-center">
      <div class="container">
        <img src="/images/norris-hor.png" class="logo-norris" title="Norris" alt="Norris Logo" />
          <h1><%= chart.getSettings().title %></h1>
      </div>
    </header>
    <div class="returnlink header-margin text-center"><a href="../../">Torna alla pagina iniziale</a></div>
    <div class="container content" style="height:600px">
        <chuck-<%= chart.getType() %> chart-endpoint="<%= endpoint %>" chart-id="<%= chart.getId() %>"></chuck-<%= chart.getType() %>>
    </div>
    <footer class="text-center">
          <img src="/images/kt-trademark.png" alt="Kaizen Team Trademark" title="Kaizen Team" height="100px" />
    </footer>
    </body>
</html>
